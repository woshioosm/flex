{
	public class StrokeStyle
	{
		public var _thickness:Number;
		public var _color:Number;
		public var _alpha:Number;
		
		public function StrokeStyle(thickness:Number,color:Number,alpha:Number)
		{
			this._thickness=thickness;
			this._color=color;
			this._alpha=alpha;
		}

	}
}